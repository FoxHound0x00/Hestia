{
    "manifest_version": 2,
    "name": "Hestia",
    "version": "1.0",
  
    "description": "Search your bookmarks easily",
    "icons": {
      "48": "images/icon.png"
    },
  
    "permissions": ["bookmarks"],
  
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
  
    "browser_action": {
      "default_title": "Hestia",
      "default_popup": "index.html"
    }
  }
  